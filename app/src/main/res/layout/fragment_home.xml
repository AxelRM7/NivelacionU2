<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">
    <ImageButton
        android:id="@+id/wifi"
        android:layout_width="180dp"
        android:layout_height="180dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/blueto"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/ic_baseline_signal_wifi_4_bar_24" />

    <ImageButton
        android:id="@+id/blueto"
        android:layout_width="180dp"
        android:layout_height="180dp"
        app:layout_constraintLeft_toRightOf="@id/wifi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/ic_baseline_bluetooth_disabled_24" />

    <ImageButton
        android:id="@+id/brillo"
        android:layout_width="180dp"
        android:layout_height="180dp"
        app:layout_constraintTop_toBottomOf="@id/wifi"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/lampara"
        android:src="@drawable/ic_baseline_brightness_7_24" />

    <ImageButton
        android:id="@+id/lampara"
        android:layout_width="180dp"
        android:layout_height="180dp"
        app:layout_constraintTop_toBottomOf="@id/blueto"
        app:layout_constraintLeft_toRightOf="@id/brillo"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_baseline_flashlight_on_24" />

    <ImageButton
        android:id="@+id/avion"
        android:layout_width="180dp"
        android:layout_height="180dp"
        app:layout_constraintTop_toBottomOf="@id/brillo"
        app:layout_constraintRight_toLeftOf="@id/sonido"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/ic_baseline_airplanemode_inactive_24" />

    <ImageButton
        android:id="@+id/sonido"
        android:layout_width="180dp"
        android:layout_height="180dp"
        app:layout_constraintTop_toBottomOf="@id/lampara"
        app:layout_constraintLeft_toRightOf="@id/avion"
        app:layout_constraintRight_toRightOf="parent"
        android:src="@drawable/ic_baseline_ring_volume_24" />


</androidx.constraintlayout.widget.ConstraintLayout>